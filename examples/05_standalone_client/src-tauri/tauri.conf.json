{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "",
    "distDir": "www"
  },
  "package": {
    "productName": "Trame Client",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "client.only.example",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "localhost",
          "windows": ["main"],
          "scheme": "http",
          "enableTauriAPI": true
        }
      ],
      "csp":  "default-src 'self' 'unsafe-inline' ws: *; script-src 'unsafe-eval' 'self';"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Trame",
        "width": 800,
        "visible": false
      }
    ],
    "cli": {
      "description": "Connect to remote service",
      "longDescription": "",
      "beforeHelp": "",
      "afterHelp": "", 
      "args": [
        {
          "name": "url",
          "takesValue": true
        }
      ]
    }
  }
}
